                    "code": "file:../output/basic-features.wasm"
                },
                "address:an_account": {
                    "nonce": "1",
                    "balance": "0"
                },
                "sc:value-storage": {
                    "nonce": "1",
                    "balance": "0",
                    "code": "file:../../value_storage/output/value_storage.wasm"
                }
            }
        },
        {
            "step": "scCall",
            "id": "fill destination set mapper",
            "tx": {
                "from": "address:an_account",
                "to": "sc:value-storage",
                "value": "0",
                "function": "fill_set_mapper",
                "arguments": [
                    "0x0a"
                ],
                "gasLimit": "50,000,000",
                "gasPrice": "0"
            },
            "expect": {
                "out": [],
                "status": "0",
                "logs": "*",
                "gas": "*",
                "refund": "*"
            }
