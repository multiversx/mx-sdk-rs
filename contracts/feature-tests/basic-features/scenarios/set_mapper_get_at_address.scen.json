        {
            "step": "scCall",
            "id": "set other contract address",
            "tx": {
                "from": "address:an_account",
                "to": "sc:basic-features",
                "value": "0",
                "function": "set_other_contract_address",
                "arguments": [
                    "sc:value-storage"
                ],
                "gasLimit": "50,000,000",
                "gasPrice": "0"
            },
            "expect": {
                "out": [],
                "status": "0",
                "logs": "*",
                "gas": "*",
                "refund": "*"
            }
        },
        {
            "step": "scCall",
            "id": "contains at address",
            "tx": {
                "from": "address:an_account",
                "to": "sc:basic-features",
                "value": "0",
                "function": "contains_at_address_endpoint",
                "arguments": [
                    "0x05"
                ],
                "gasLimit": "50,000,000",
                "gasPrice": "0"
            },
            "expect": {
                "out": ["0x01"],
                "status": "0",
                "logs": "*",
                "gas": "*",
                "refund": "*"
            }
        }
